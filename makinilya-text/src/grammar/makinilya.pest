WHITESPACE           = _{ " " }
identifier           =  { "_"? ~ ASCII_ALPHA+ ~ ("."? ~ (ASCII_ALPHANUMERIC | "_"))* }
string_interpolation =  { "{{" ~ (!"}}" ~ identifier) ~ "}}" }
text_content         = ${ (!"{{" ~ ANY)+ }
expression           =  { string_interpolation | text_content }
makinilya            =  { SOI ~ expression* ~ EOI }
